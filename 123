entered_number = int(input('Введите число от 0 до 10: '))
if entered_number < 10 and entered_number > 0:
    print(entered_number ** 2)
elif entered_number == 10 and entered_number == 0:
    print(entered_number ** 2)
else:
    entered_number = int(input('Введите число от 0 до 10: '))
if entered_number < 10 and entered_number > 0:
    print(entered_number ** 2)
elif entered_number == 10 and entered_number == 0:
    print(entered_number ** 2)
    
    
#entered_number = int(input('Введите число от 0 до 10: '))
while entered_number < 10 and entered_number > 0: 
    print(int(input('Упс! Число не верно')))
    entered_number = int(input('Введите число от 0 до 10: '))


